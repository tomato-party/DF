BFvdcE2i"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 모델 학습\n","model.fit(trainX, trainY, epochs=100, batch_size=16, verbose=2)"],"metadata":{"id":"c5dxr0HQcHsl"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 예측\n","train_predict = model.predict(trainX)\n","test_predict = model.predict(testX)"],"metadata":{"id":"v1ohmbj6cKN3"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 예측 결과 역정규화\n","train_predict = scaler.inverse_transform(train_predict)\n","trainY = scaler.inverse_transform([trainY])\n","test_predict = scaler.inverse_transform(test_predict)\n","testY = scaler.inverse_transform([testY])"],"metadata":{"id":"7efYconScOH-"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 예측 결과 평가\n","train_score = math.sqrt(mean_squared_error(trainY[0], train_predict[:, 0]))\n","print('Train RMSE: %.2f' % train_score)\n","test_score = math.sqrt(mean_squared_error(testY[0], test_predict[:, 0]))\n","print('Test RMSE: %.2f' % test_score)"],"metadata":{"id":"_GJ3wI-GcRnX"},"execution_count":null,"outputs":[]}]}